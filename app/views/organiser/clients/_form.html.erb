<%= form_for [:organiser, @client] do |f| %>
    <%= render "shared/errors", model: @client %>

    <%= f.label :name %>
    <br/>
    <%= f.text_field :name %>
    <br/>

    <%= f.label :email %>
    <br/>
    <%= f.email_field :email %>
    <br/>

    <%= f.label :phone %>
    <br/>
    <%= f.text_field :phone %>
    <br/>

    <%= f.label :address %>
    <br/>
    <%= f.text_field :address %>
    <br/>

    <%= f.label :postcode %>
    <br/>
    <%= f.text_field :postcode %>
    <br/>

    <%= f.submit %>
<% end %>